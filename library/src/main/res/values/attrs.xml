<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FABProgressCircleGlobal">
        <attr name="arcColor" format="color"/>
        <attr name="arcWidth" format="dimension"/>
        <attr name="circleSize" format="enum">
            <enum name="normal" value="1"/>
            <enum name="mini" value="2"/>
        </attr>
        <attr name="roundedStroke" format="boolean"/>
        <attr name="completeArcDuration" format="integer"/>
        <attr name="rotationArcDuration" format="integer"/>
        <attr name="growArcDuration" format="integer"/>
        <attr name="shrinkArcDuration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="FABProgressCircle" parent="FABProgressCircleC">
        <attr name="reusable" format="boolean"/>
        <attr name="finalIcon" format="reference"/>
        <attr name="finalIconDuration" format="integer"/>
        <attr name="showFinalIcon" format="boolean"/>
    </declare-styleable>
</resources>